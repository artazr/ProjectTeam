<!DOCTYPE html>
<html lang="fr">
    <head>
        {% block head %}{% endblock %}

            <title>{% block title %}{% endblock %} - PROJECTeam</title>
                <meta http-equiv="content-type" content="text/html; charset=utf-8" />
                <meta name="description" content="Site officiel de Projecteam organisé par Junior ISEP" />
                <meta name="keywords" content="" />
                <meta name="viewport" content="width=device-width, initial-scale=1.0,maximum-scale=1">
                    <link href="{{ asset('css/style.css') }}" rel="stylesheet" type="text/css" />
        


                <link rel="stylesheet" href="{{ asset('css/flipclock.css') }}">
                <script src="{{ asset('js/jquery.min.js') }}"></script>
                <script src="{{ asset('js/flipclock.js') }}"></script>
                    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" /> 
       
        <style type="text/css">
            @font-face {
                font-family: "Perso";
                 src: url('{{ asset('fonts/fontawesome-webfont.ttf') }}');
                }

            #nav li{
                font-family: "Perso";
            }

            .clock{
                margin-left:25%;
                margin-right:25%;
            }
        </style>
        {% block stylesheet %}{% endblock %}

    </head>

    <body class="homepage">
        <!--Header -->
        <header>
            <div id="header-wrapper">
                <div id="header" class="container">
                    <h1 id="logo"><a href="{{ path('acme_core_backoffice') }}">PROJECTeam</a></h1>
                    <!-- Nav -->
                    <nav id="nav">
                    <br />                
                        <ul>
                            <li><a href="{{ path('acme_core_modif') }}">Modifier les pages</a></li>
                            <li><a href="{{ path('acme_core_entreprise') }}">Voir les entreprises inscrites</a></li>
                            <li><a href="{{ path('acme_core_inscrit') }}">voir les utilisateurs inscrits</a></li>
                            <li><a href="{{ path('acme_core_feedbackuser') }}">Voir les feedback des utilisateurs</a>
                            <li style="float:right;"> <a href="{{ path('acme_core_logout') }}">se déconnecter</a> </li>
                        </ul>
                    </nav>
                </div>
                <br />
                <!-- Countdown -->
                <div class="clock"></div>
                    <script type="text/javascript">
                        // Grab the current date
                             var currentDate = new Date();
                        // Set some date in the past. In this case, it's always been since Jan 1
                             var futureDate  = new Date(currentDate.getFullYear(2015), 4, 23);
                        // Calculate the difference in seconds between the future and current date
                             var diff = futureDate.getTime() / 1000 - currentDate.getTime() / 1000;
                         var clock = $('.clock').FlipClock(diff, {
                         countdown: true,
                         clockFace: 'DailyCounter',
                        });
                    </script>
            </div>
        </header>
        <!-- contenu modifiable par page -->
        {% block body %}{% endblock %}
        <!--footer-->      
            <footer id="footer">
                <div class="container">
                    <div class="row double">
                        <div class="6u">
                            <div class="row collapse-at-2">

{% extends '::base.html.twig' %}
{% block title %}
Contact Entreprise
{% endblock %}
 {% block stylesheet %}
 <link rel="stylesheet" href="{{ asset('css/perso.css') }}">

 {% endblock %}
{% block body %}

<form action="{{ path('acme_core_contactentreprise') }}" {{ form_enctype(form) }} method="POST">
    <table>
		
		<td> Nom de l'entreprise : </td>
		<td> {{ form_widget(form.nom) }}</td>
		
		
		<td> Votre logo : </td>
		<td> {{ form_widget(form.imageFile) }}</td>
		
		<td> Email de contact : </td>
		<td> {{ form_widget(form.email) }}</td>
		
		<td> Remarques , informations et participations </td>
		<td> {{ form_widget(form.remarque) }}</td>


	</table>
    <div>
        <button type="submit" class="btn btn-primary">Submit</button>
        <button type="button" class="btn btn-default">Cancel</button>
    </div>
</form>

{% endblock %}

